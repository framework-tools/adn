{
    "compilerOptions": {
        "incremental": true,
        "target": "ESNext",
        "module": "CommonJS",
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "strict": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "node"
        ],
    }
}